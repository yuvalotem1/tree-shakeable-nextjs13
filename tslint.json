{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ],
    "no-unused-expression": false,
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      200
    ],
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "max-classes-per-file": false,
    "arrow-parens": false,
    "no-bitwise": false,
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "class-name": true,
    "no-shadowed-variable": false,
    "no-empty": false,
    "no-empty-interface": {
      "severity": "warning"
    },
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    }
  },
  "linterOptions": {
    "exclude": [
      "src/**/*.spec.ts"
    ]
  },
  "rulesDirectory": []
}
